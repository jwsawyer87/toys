# This is a fragment of a bash profile that I liked to use
# 
# Remember that bash looks for profile scripts in the following order:
#   $HOME/.bash_profile
#   $HOME/.bash_login
#   $HOME/.profile

export PAGER=less
export EDITOR=vi

export PATH="$PATH:$HOME/bruno/bin"
export PS1=PS1='\u@\H \W> '

export BASHRC=$HOME/bruno/toys/misc/.bashrc
. $BASHRC
